---
import Users from "../components/Users/Users.astro";
import Layout from "../layouts/Layout.astro";
---
<script>
  // Redirige a /wednesday si ya hay usuario en localStorage
  if (localStorage.getItem("user")) {
    const basePath = '';
    window.location.replace(`${basePath}/wednesday`);
  }
</script>

<Layout hideUserAvatar="true">
  <Users />
</Layout>
